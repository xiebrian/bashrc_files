[user]
	email = brianxie@mit.edu
	name = xiebrian
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
	grep = auto
	ui = true
[alias]
    graph = log --graph --oneline --decorate --color --all
[credential]
    helper = store
[push]
    default = matching
[core]
	pager = diff-so-fancy | less --tabs=4 -RFX
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 11
	frag = magenta bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
